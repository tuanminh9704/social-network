extends ../../layouts/user.pug

block main 
  .container
    //- Phần header
    .header-profile
      .cover-photo
        img(src="https://giaydabongtot.com/wp-content/uploads/2020/10/Hinh-nen-ronaldo-cr7-may-tinh-laptop-2.jpg")
      .profile-info
        img.avatar(src="https://assets.manutd.com/AssetPicker/images/0/0/10/126/687707/Legends-Profile_Cristiano-Ronaldo1523460877263.jpg" alt="avatar")
        h1.user-name Tên Người Dùng

    //- Chia thành 2 cột
    .row
      .col-md-4
        //- Thông tin cơ bản
        .inner-about
          h2 GIới thiệu 
          .inner-home-town   
              p Quê quán: Hà Nội
          .inner-phone-number 
              p Số điện thoại: 0123456789
          .inner-email  
              p Email: example@example.com
      .col-8.post-profile
        .inner-body-post
          .inner-posts-wrap.p-4.border
            .inner-posts-head.mb-3
              .inner-title
                .inner-avatar
                  img(src="data:image/jpeg;base64,", alt="Avatar")
                .inner-fullname Vũ Tuấn Minh
              .inner-desc
                p.inner-content Lorem ipsum dolor sit, amet consectetur adipisicing elit. Atque at sed non quos eos dolore architecto alias, quam nulla rem a blanditiis incidunt dolorem voluptas. Quo unde facilis quis possimus.
            .inner-posts-body 
              .inner-image 
                img(src="https://giaydabongtot.com/wp-content/uploads/2020/10/Hinh-nen-ronaldo-cr7-may-tinh-laptop-3-scaled-2048x1280.jpg")
              .inner-posts-like 10
            .inner-button 
              div(class="button-like")
                i.fa-regular.fa-thumbs-up
                span() Thích
              div(class="button-comment")
                i.fa-regular.fa-comment
                span Comment
        .inner-body-post
          .inner-posts-wrap.p-4.border.bg-light.rounded
